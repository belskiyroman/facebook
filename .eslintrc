{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": false,
    "es6": true
  },
  "plugins": [
    "import",
    "react",
    "babel"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": 2,
    "no-console":0,
    "quotes": ["error", "single"]
  }
}
